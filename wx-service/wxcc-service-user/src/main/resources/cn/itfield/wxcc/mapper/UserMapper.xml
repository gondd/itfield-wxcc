<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.itfield.wxcc.mapper.UserMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.itfield.wxcc.domain.User">
        <id column="id" property="id" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
        <result column="third_uid" property="thirdUid" />
        <result column="phone" property="phone" />
        <result column="email" property="email" />
        <result column="nick_name" property="nickName" />
        <result column="bit_state" property="bitState" />
        <result column="sec_level" property="secLevel" />
        <result column="login_id" property="loginId" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, create_time, update_time, third_uid, phone, email, nick_name, bit_state, sec_level, login_id
    </sql>

</mapper>
