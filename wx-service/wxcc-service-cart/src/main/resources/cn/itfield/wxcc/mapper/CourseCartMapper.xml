<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.itfield.wxcc.mapper.CourseCartMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.itfield.wxcc.domain.CourseCart">
        <id column="id" property="id" />
        <result column="course_name" property="courseName" />
        <result column="course_pic" property="coursePic" />
        <result column="course_price" property="coursePrice" />
        <result column="count" property="count" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
        <result column="status" property="status" />
        <result column="course_note" property="courseNote" />
        <result column="course_id" property="courseId" />
        <result column="login_id" property="loginId" />
        <result column="grade_name" property="gradeName" />
        <result column="old_course_price" property="oldCoursePrice" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, course_name, course_pic, course_price, count, create_time, update_time, status, course_note, course_id, login_id, grade_name, old_course_price
    </sql>

</mapper>
